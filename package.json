{
    "name": "crypto-payments-challenge",
    "description": "At a crypto payments company receives thousands of deposits from customers per day. This test is designed to test your ability to work with a transaction set that could get returned by a blockchain daemon like bitcoind.",
    "version": "1.0.0",
    "main": "dist/app.js",
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ext": "ts",
        "ignore": [
            "*.test.ts"
        ],
        "delay": "3",
        "execMap": {
            "ts": "ts-node"
        }
    },
    "scripts": {
        "start": "nodemon --watch",
        "test": ""
    },
    "dependencies": {
        "axios": "^1.6.8",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "pg": "^8.11.5",
        "picocolors": "^1.0.0",
        "typeorm": "^0.3.20"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "eslint": "^9.0.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.0",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    },
    "keywords": [
        "challenge",
        "payments"
    ],
    "author": "Jessica Elberg",
    "license": "ISC"
}
